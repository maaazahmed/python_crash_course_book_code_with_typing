    "def greet_users(names:list[str])->None:\n",
    "    \"\"\"Print a simple greeting to each user in the list.\"\"\"\n",
    "    for name in names:\n",
    "        msg:str = f\"Hello, {name}\"\n",
    "        print(msg)\n",
    "\n",
    "\n",
    "usernames:list[str] = ['hannah', 'ty', 'margot']\n",
    "\n",
    "greet_users(usernames)\n"
   ]
